        <BeanShellSampler guiclass="BeanShellSamplerGui" testclass="BeanShellSampler" testname="BeanShell Sampler" enabled="true">
          <stringProp name="BeanShellSampler.query">import java.text.SimpleDateFormat;
import java.util.Calendar;
import java.util.Date;

try{
    Date date =new Date();  //获取当前时间
    SimpleDateFormat sf = new SimpleDateFormat(&quot;yyyy-MM-dd&apos;T&apos;HH:mm:ss.SSS Z&quot;);
    String nowDate = sf.format(date);

    Calendar cal = Calendar.getInstance();
    cal.setTime(sf.parse(nowDate));
    cal.add(Calendar.DAY_OF_YEAR,+1);  //后一天
    String orderDate = sf.format(cal.getTime());
    
    cal.add(Calendar.DAY_OF_YEAR,+1);  //怎么还是加1
    String senderDate = sf.format(cal.getTime());

    cal.add(Calendar.DAY_OF_YEAR,-3);  //昨天
    String zuoDate = sf.format(cal.getTime()); 

    vars.put(&quot;orderDate&quot;,orderDate);
    vars.put(&quot;senderDate&quot;,senderDate);
    vars.put(&quot;zuoDate&quot;,zuoDate);
    vars.put(&quot;nowDate&quot;,nowDate); //今天
    
}
catch(Exception e){
    
}</stringProp>
