          <BeanShellAssertion guiclass="BeanShellAssertionGui" testclass="BeanShellAssertion" testname="BeanShell断言" enabled="true">
            <stringProp name="BeanShellAssertion.query">import base.BaseBeanShell;
import base.PreviousData;
import net.minidev.json.JSONObject;
import net.minidev.json.JSONValue;
import net.minidev.json.JSONArray;       
        
        
        String jsonContent = prev.getResponseDataAsString();  //获取返回的json
        JSONObject jsonObj=(JSONObject)JSONValue.parse(jsonContent); //把返回字符串变为json对象
        int code = (int)jsonObj.get(&quot;code&quot;);
        String msg = (String) jsonObj.get(&quot;msg&quot;);

        Object data = null;
        JSONArray items = null;
        String policyInfo =null;
        if(jsonObj.containsKey(&quot;data&quot;)){
            Object dataObject=jsonObj.get(&quot;data&quot;);
            if(dataObject instanceof JSONObject){
                data= jsonObj.get(&quot;data&quot;);
            }else if(dataObject instanceof JSONArray){
                data=jsonObj.get(&quot;data&quot;);
            }
            log.info(&quot;data&quot; + data);
            items = (JSONArray)((JSONObject)jsonObj.get(&quot;data&quot;)).get(&quot;items&quot;);
            log.info(&quot;items&quot;+ items);
            if(items != null){
                for(int i =0;i&lt;items.size();i++){
                    JSONObject item = (JSONObject)items.get(i);
                    vars.put(&quot;policyInfo&quot;+i,String.valueOf(item.get(&quot;policyInfo&quot;)));
                    log.info(String.valueOf(item.get(&quot;policyInfo&quot;)));
                    policyInfo = String.valueOf(item.get(&quot;policyInfo&quot;));
                    log.info(policyInfo);
                }

            }

        }else{
            Failure=true;
            FailureMessage=&quot;返回结果中没有data数据&quot;;
        }

        //判断返回值是否和预期一致
        if (code != 1) {
            //把断言失败置为真，即用例失败，并在结果树中显示FailureMessage
            Failure = true;
            FailureMessage = &quot;statusCode的返回值有误&quot;;
        }
        log.info(Failure+&quot;   ---   &quot;+FailureMessage);

        if (!msg.equals(&quot;成功&quot;)) {
            //把断言失败置为真，即用例失败，并在结果树中显示FailureMessage
            Failure = true;
            FailureMessage = &quot;message与实际值不一致&quot;;
        }

        if (items == null || items.size() == 0){
            Failure=true;
            FailureMessage=&quot;返回结果中没有items数据&quot;;
        }
        log.info(Failure+&quot;   ---   &quot;+FailureMessage);

        if(!policyInfo.equals(&quot;UPDATA TEST NO1~~~~~~~~~~~~~~&quot;)){

            Failure=true;
            FailureMessage=&quot;返回结果中没有政策数据&quot;;
        }
        log.info(Failure+&quot;   ---   &quot;+FailureMessage);</stringProp>
